{
  "name": "@xjectro/react-utils",
  "private": false,
  "version": "1.1.33",
  "description": "A React UI component library for Xjectro with Tailwind CSS, Radix UI, custom hooks, and design system utilities",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xjectro/npm-packages.git"
  },
  "homepage": "https://github.com/Xjectro/npm-packages#readme",
  "bugs": {
    "url": "https://github.com/Xjectro/npm-packages/issues"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "tailwindcss",
    "radix-ui",
    "typescript",
    "design-system",
    "hooks",
    "xjectro"
  ],
  "author": "xjectro",
  "files": [
    "dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "release": "npm version patch && npm publish"
  },
  "exports": {
    "./hooks/*": {
      "types": "./dist/hooks/*.d.ts",
      "import": "./dist/hooks/*.mjs",
      "require": "./dist/hooks/*.cjs"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.mjs",
      "require": "./dist/lib/*.cjs"
    }
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "overrides": {
    "cmdk": {
      "@types/react": "^18.0.0",
      "@types/react-dom": "^18.0.0"
    }
  }
}
